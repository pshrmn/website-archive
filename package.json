{
  "name": "weplay",
  "version": "0.8.0",
  "description": "Plays games with other people",
  "main": "server.js",
  "scripts": {
    "css": "npm run sass & npm run prefix",
    "lint": "eslint dev --ext .jsx",
    "prefix": "postcss --use autoprefixer public/css/games.css public/css/index.css --dir public/css/",
    "preversion": "npm test",
    "sass": "node-sass dev/scss/** -o public/css/**",
    "test": "mocha specs --require specs/helpers/chai.js",
    "webpack": "webpack & webpack --config=webpack.config.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psherman/weplay.git"
  },
  "author": "Paul Sherman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psherman/weplay/issues"
  },
  "engines": {
    "node": "4.x",
    "npm": "3.x"
  },
  "homepage": "https://github.com/psherman/weplay",
  "dependencies": {
    "express": "^4.10.2",
    "nunjucks": "^1.3.4",
    "socket.io": "^1.3.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "babel-eslint": "^4.1.2",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "eslint": "^1.4.1",
    "eslint-plugin-react": "^3.3.2",
    "mocha": "^2.2.5",
    "postcss-cli": "^2.1.0",
    "react-tools": "^0.13.3",
    "sinon": "^1.16.1",
    "webpack": "^1.12.0"
  }
}
